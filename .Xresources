!URxvt.shading:50
!URxvt.transparent: true
URxvt*font: xft:Terminess\ Powerline:size=12
URxvt*scrollTtyOutput: false
!URxvt*foreground: white
URxvt.scrollBar: false
!urxvt*background: rgba:0000/0000/0200/c800
!urxvt*depth: 32
!urxvt*background: rgba:2000/2000/2000/cccc

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
rofi.separator-style:                solid
rofi.ssh-client:                     ssh
rofi.ssh-command:                    {terminal} -e "{ssh-client} {host}"
rofi.color-normal:                    argb:00000000,  #daba8b, argb:00000000, #daba8b, #1b2b34 
rofi.color-urgent:                   argb:00000000,  #845336 , argb:00000000,  #845336 ,  #1B2B34
rofi.color-active:                   argb:00000000,  #191716, argb:00000000,  #191716,  #1B2B34
rofi.color-window:                   argb:ee191716,  #daba8b, #daba8b 



URxvt*urgentOnBell: true
! special
*.foreground:   #daba8b
*.cursorColor:  #daba8b
URxvt*depth: 32
URxvt*background: [93]#191716
! black
*.color0:       #0f0e0d
*.color8:       #383332

! red
*.color1:       #845336
*.color9:       #8c4f4a

! green
*.color2:       #57553c
*.color10:      #898471

! yellow
*.color3:       #a17e3e
*.color11:      #c8b491

! blue
*.color4:       #43454f
*.color12:      #65788f

! magenta
*.color5:       #604848
*.color13:      #755e4a

! cyan
